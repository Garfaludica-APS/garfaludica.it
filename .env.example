MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FRON_ADDRESS="no-reply@garfaludica.it"
MAIL_FROM_NAME="Garfaludica APS"
