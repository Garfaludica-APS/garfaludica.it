APP_NAME="Garfaludica APS"
APP_ENV=local
APP_DOMAIN=localhost
APP_DEBUG=true
APP_URL=http://localhost
# APP_PORT=80
FRONTEND_URL=http://localhost

APP_KEY=

APP_TIMEZONE=UTC
APP_LOCALE=it
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=cache
APP_MAINTENANCE_STORE=redis


# === AUTH ===

AUTH_GUARD=web
AUTH_PASSWORD_BROKER=users
# AUTH_MODEL=
AUTH_PASSWORD_RESET_TOKEN_TABLE=password_reset_tokens
AUTH_PASSWORD_TIMEOUT=10800


# === BROADCAST ===

BROADCAST_CONNECTION=log


# === CACHE ===

CACHE_STORE=redis
# CACHE_PREFIX=
REDIS_CACHE_CONNECTION=cache
REDIS_CACHE_LOCK_CONNECTION=default
DYNAMODB_CACHE_TABLE=cache
# DYNAMODB_ENDPOINT=
# DB_CACHE_CONNECTION=
DB_CACHE_TABLE=cache
# DB_CACHE_LOCK_CONNECTION=
# DB_CACHE_LOCK_TABLE=


# === DATABASE ===

DB_CONNECTION=mariadb
# DB_URL=
# DB_SOCKET=
DB_HOST=mariadb
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=sail
DB_PASSWORD=
DB_FOREIGN_KEYS=true
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci
# FORWARD_DB_PORT=3306


# === DEBUGBAR ===

DEBUGBAR_OPEN_STORAGE=true
DEBUGBAR_LOCAL_SITES_PATH=


# === FILESYSTEM / AWS ===

FILESYSTEM_DISK=local
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=
# AWS_BUCKET=
# AWS_URL=
# AWS_ENDPOINT=
AWS_USE_PATH_STYLE_ENDPOINT=false

# === HASHING ===

HASH_DRIVER=bcrypt
BCRYPT_ROUNDS=12
HASH_VERIFY=true
ARGON_MEMORY=65536
ARGON_THREADS=1
ARGON_TIME=4



# === INERTIA ===

INERTIA_SSR_ENABLED=true


# === LOGGING ===

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_DEPRECATIONS_TRACE=false
LOG_STACK=single
LOG_LEVEL=debug
LOG_DAILY_DAYS=14
# LOG_SLACK_WEBHOOK_URL=
LOG_SLACK_USERNAME="Laravel Log"
LOG_SLACK_EMOJI=":boom:"
